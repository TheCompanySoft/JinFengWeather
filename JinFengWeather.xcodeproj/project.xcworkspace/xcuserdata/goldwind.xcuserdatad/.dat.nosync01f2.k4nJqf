- (NSInteger)tableView:(UITableView *)tableView numberOfRowsInSection:(NSInteger)section {
    if (section==0) {
        return 2;
    }
    
    
    if (_flag == 1) {
        if (section== 2) {
            return 1;
        }
        if (section == 3) {
            return 1;
        }
    }else{
        if (section == 2) {
            return 1;
        }
    }
    
    
    
    if (section==1) {
        if (isresponse==NO)
        {
            return 1;
            isresponse=YES;
        }
    }else{
        return 0;
        isresponse=NO;
    }
    
    return 0;
}
