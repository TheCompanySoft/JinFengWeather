//section的高度
-(CGFloat)tableView:(UITableView *)tableView heightForHeaderInSection:(NSInteger)section{
    if (section==0) {
        return 0;
    } if (section==1) {
        return [UIUtils getWindowHeight]/142*11;
    }else if (_flag == 1){
        if(section == 2){
            return 0;
        }else{
            return [UIUtils getWindowHeight]/10*3-30*[UIUtils getWindowHeight]/568;
        }
    }else{
        return [UIUtils getWindowHeight]/10*3-30*[UIUtils getWindowHeight]/568;
    }
